@using MainApp.Components.Deployment
@using Camunda.Http.Api

@inject AppData AppData

@page "/deployments/create"

<Container>
    <Row>
        <Column>
            <Heading Size="HeadingSize.Is1">Create a Deployment</Heading>
        </Column>
    </Row>
    <Row>
        <Column>
            <CreateDeploymentForm Camunda="@camunda" />
        </Column>
    </Row>
</Container>


@code {
    CamundaClient camunda;

    protected override void OnInitialized()
    {
        camunda = AppData.CamundaClient;
    }

}
