@using Camunda.Http.Api
@using MainApp.Components.Deployment

@inject AppData AppData

@page "/deployments"

<Container>
    <Row>
        <Column>
            <Heading Size="HeadingSize.Is1">Deployments</Heading>
        </Column>
    </Row>
    <Row>
        <Column>
            <DeploymentDataGrid camunda="@camunda" pageSize="15" />
        </Column>
    </Row>
</Container>


@code {
    CamundaClient camunda;

    protected override void OnInitialized()
    {
        camunda = AppData.CamundaClient;
    }

}
